{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "GITHUB_PACKAGES_TOKEN",
    "AAC_GRAPHQL_URL",
    "IMC_GRAPHQL_URL",
    "SGC_GRAPHQL_URL",
    "SXP_GRAPHQL_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "codegen": {
      "outputs": ["generated/**", "**/generated/**"]
    }
  }
}
